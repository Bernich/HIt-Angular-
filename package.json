{
  "name": "hive-media",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "server": "json-server server/db.json",
    "server:all": "concurrently \"npm run server\" \"ng serve\"",
    "start": "node dist/hive-media/server/main.js",
    "build": "ng build --prod",
    "build:stats": "ng build --prod --stats-json",
    "analyze": "webpack-bundle-analyzer dist/hive-media/stats-es2015.json",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "compodoc": "npx compodoc -p tsconfig.app.json src",
    "deploy": "ng build --prod && npm run build:ssr && gcloud app deploy",
    "dev:ssr": "ng run hive-media:serve-ssr",
    "serve:ssr": "node dist/hive-media/server/main.js",
    "build:ssr": "ng build --prod && ng run hive-media:server:production",
    "prerender": "ng run hive-media:prerender"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.1.12",
    "@angular/cdk": "^9.1.3",
    "@angular/common": "~9.1.12",
    "@angular/compiler": "~9.1.12",
    "@angular/core": "~9.1.12",
    "@angular/forms": "~9.1.12",
    "@angular/localize": "~9.1.12",
    "@angular/material": "^9.1.3",
    "@angular/platform-browser": "~9.1.12",
    "@angular/platform-browser-dynamic": "~9.1.12",
    "@angular/platform-server": "~9.1.12",
    "@angular/router": "~9.1.12",
    "@ng-bootstrap/ng-bootstrap": "^9.1.0",
    "@ng-web-apis/common": "^1.9.0",
    "@ng-web-apis/universal": "^1.9.1",
    "@nguniversal/express-engine": "^9.1.1",
    "@trademe/ng-defer-load": "^8.2.1",
    "bootstrap": "^5.0.0",
    "concurrently": "^5.2.0",
    "core-js": "^3.12.1",
    "express": "^4.15.2",
    "hammer-timejs": "^1.1.0",
    "hammerjs": "^2.0.8",
    "lodash": "^4.17.21",
    "lottie-web": "^5.7.3",
    "material": "^0.4.3",
    "moment": "^2.29.1",
    "ngx-bootstrap": "^6.2.0",
    "ngx-infinite-scroll": "^10.0.1",
    "rxjs": "^6.6.3",
    "tslib": "^1.10.0",
    "uuid": "^8.3.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.12",
    "@angular/cli": "~9.1.12",
    "@angular/compiler-cli": "~9.1.12",
    "@nguniversal/builders": "^9.1.1",
    "@types/express": "^4.17.0",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~3.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~3.8.3",
    "webpack-bundle-analyzer": "^4.4.1",
    "@angular-devkit/architect": ">= 0.900 < 0.1200",
    "fuzzy": "^0.1.3",
    "inquirer": "^6.2.2",
